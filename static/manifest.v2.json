{
	"manifest_version": 2,
	"permissions": [
		"storage",
		"proxy",
		"cookies",
		"webRequest",
		"webRequestBlocking",
		"tabs",
		"*://sage.party/*",
		"*://*.sage.party/*",
		"*://store.steampowered.com/*",
		"*://steamcommunity.com/*",
		"*://help.steampowered.com/*",
		"*://steam.tv/*",
		"*://checkout.steampowered.com/*"
	],
	"background": {
		"scripts": ["background.js"]
	}
}
