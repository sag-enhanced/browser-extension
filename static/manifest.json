{
	"name": "Steam Account Generator Enhanced",
	"version": "11",
	"description": "Assistance for helping sage.party interact with Steam and other services.",
	"content_scripts": [
		{
			"matches": ["https://store.steampowered.com/join/"],
			"js": ["scripts/steam.js"],
			"all_frames": true,
			"run_at": "document_start"
		},
		{
			"matches": [
				"https://recaptcha.net/recaptcha/enterprise/anchor*",
				"https://google.com/recaptcha/enterprise/anchor*"
			],
			"js": ["scripts/recaptcha.js"],
			"all_frames": true
		},
		{
			"matches": ["https://sage.party/*"],
			"js": ["scripts/sage.js"],
			"all_frames": true,
			"run_at": "document_start"
		}
	],
	"icons": {
		"16": "icon/16x16.jpg",
		"48": "icon/48x48.jpg",
		"256": "icon/256x256.jpg"
	}
}
