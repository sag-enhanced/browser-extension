{
	"manifest_version": 3,
	"permissions": ["cookies"],
	"background": {
		"service_worker": "background.js",
		"type": "module"
	},
	"host_permissions": [
		"*://sage.party/*",
		"*://google.com/recaptcha/enterprise/*",
		"*://recaptcha.net/recaptcha/enterprise/*",
		"*://steamcommunity.com/",
		"*://store.steampowered.com/join/*",
		"*://login.steampowered.com/join/*",
		"*://.store.steampowered.com/join/*"
	],
	"minimum_chrome_version": "100"
}
